{
  "name": "bem_front-stack",
  "version": "0.4.3",
  "dependencies": {
    "jquery": "~2.2.0",
    "normalize.css": "~3.0.3",
    "vue": "^2.5.13",
    "vueify": "^9.2.4"
  },
  "devDependencies": {
