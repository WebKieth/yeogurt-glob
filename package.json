    "gulp-changed": "~1.3.0",
    "gulp-coffee": "^2.3.4",
    "gulp-coffeescript": "0.0.10",
    "gulp-cssnano": "~2.1.0",
    "gulp-data": "~1.2.1",
    "gulp-eslint": "~1.1.1",
    "gulp-htmlmin": "~1.3.0",
    "gulp-if": "~2.0.0",
    "gulp-imagemin": "~2.4.0",
    "gulp-load-plugins": "~1.2.0",
    "gulp-newer": "~1.1.0",
    "gulp-notify": "~2.2.0",
    "gulp-plumber": "~1.0.1",
